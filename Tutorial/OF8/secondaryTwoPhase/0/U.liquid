FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      U.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type               fixedValue;
        value              uniform (0 0 0.02);  
    }
    outlet
    {
	type               pressureInletOutletVelocity;
        phi                phi.liquid;
        value              $internalField;
    }
    wall_cathode
    {
        type               noSlip;
    }
    wall_anode
    {
        type               noSlip;
    }
    wall_isolation
    {
    	type               noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
}

